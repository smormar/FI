<?xml version='1.0' encoding='utf-8'?>
  <widget id="com.smorini.FI" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FI</name>
    <description>
        Finanzas Internacionales. App de apoyo a la asignatura en GCF de ULL
    </description>
    <author email="smorini@ull.edu.es">
        Finanzas Internacionales
    </author>
    <preference name="permissions" value="none" />
<preference name="android-minSdkVersion" value="14" />
    <icon src="icon.png" />
	<splash src="splash.png" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>

</widget>
